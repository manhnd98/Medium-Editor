{
  "name": "medium-editor",
  "version": "1.0.0",
  "description": "A clone version of medium editor",
  "main": "index.js",
  "scripts": {
    "start": "webpack serve --mode=development --watch",
    "start:error": "webpack serve --mode=development --debug-webpack",
    "build:dev": "webpack --mode=development",
    "build:prod": "webpack --mode=production",
    "dev": "webpack serve --mode=development --watch",
    "test": "jest --env=jsdom",
    "coverage": "jest --coverage",
    "generate-icons": "svg-to-ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/manhnd98/Medium-Editor.git"
  },
  "author": "manhnd",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/manhnd98/Medium-Editor/issues"
  },
  "homepage": "https://github.com/manhnd98/Medium-Editor#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/uuid": "^8.3.0",
    "autoprefixer": "^10.0.1",
    "css-loader": "^5.0.0",
    "expose-loader": "^1.0.1",
    "jest": "^26.6.1",
    "jest-cli": "^26.6.1",
    "mini-css-extract-plugin": "^1.2.0",
    "nyc": "^15.1.0",
    "postcss": "^8.1.4",
    "postcss-import": "^13.0.0",
    "postcss-js": "^3.0.1",
    "postcss-loader": "^4.0.4",
    "postcss-scss": "^3.0.2",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.27.0",
    "sass-loader": "^10.0.4",
    "style-loader": "^2.0.0",
    "svg-to-ts": "^6.0.0",
    "tailwindcss": "^1.9.6",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.7",
    "typescript": "^4.0.5",
    "webpack": "^5.2.0",
    "webpack-cli": "^4.1.0",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "rxjs": "^6.6.3",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslint": "^6.1.3",
    "tsyringe": "^4.4.0",
    "uuid": "^8.3.1"
  },
  "svg-to-ts": {
    "conversionType": "constants",
    "srcFiles": [
      "./src/shared/svg/*.svg"
    ],
    "outputDirectory": "./src/shared/models",
    "interfaceName": "EditorIcon",
    "typeName": "editor",
    "prefix": "editor",
    "svgoConfig": {
      "plugins": [
        {
          "cleanupAttrs": true
        }
      ]
    },
    "fileName": "icon.model",
    "additionalModelFile": "./src/shared/models",
    "compileSources": true
  }
}
